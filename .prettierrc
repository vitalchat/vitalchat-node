{
    "tabWidth": 4,
    "useTabs": true,
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "arrowParens": "always",
    "printWidth": 1000,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "strict",
    "vueIndentScriptAndStyle": true
}